@using CarInsurance2.Models
@model List<Insuree>


<div class="container">
  <div class="row justify-content-center col-lg-12 row-no-gutters">
    <h2 class="m-2 mb-4">Requested Quotes</h2>
  </div>
  <table class="table">
    <tr>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Email Address</th>
      <th scope="col">Quote</th>
    </tr>
    @foreach(var customer in Model)
    {
      <tr>
        <td>
          @customer.FirstName
        </td>
        <td>
          @customer.LastName
        </td>
        <td>
          @customer.EmailAddress
        </td>
        <td>
          @customer.Quote
        </td>
      </tr>
    }
  </table>
</div>

